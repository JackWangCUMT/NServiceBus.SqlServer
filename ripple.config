    <Dependency Name="NServiceBus" Version="4.7.0-Unstable0192" Mode="Float" />
    <Dependency Name="NServiceBus.AcceptanceTesting" Version="4.7.0-Unstable0192" Mode="Float" />
    <Dependency Name="NServiceBus.AcceptanceTests.Sources" Version="4.7.0-Unstable0192" Mode="Float" />
